<p id="notice"><%= notice %></p>

<h1>Upload Files</h1>

<table>
  <thead>
    <tr>
      <th>Cover letter</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_files.each do |upload_file| %>
      <tr>
        <td><%= upload_file.cover_letter %></td>
        <td><%= upload_file.title %></td>
        <td><%= link_to 'download', download_path(upload_file) %></td>
        <td><%= link_to 'show', upload_file %></td>
        <td><%= link_to 'Edit', edit_upload_file_path(upload_file) %></td>
        <td><%= link_to 'Destroy', upload_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload File', new_upload_file_path %>
